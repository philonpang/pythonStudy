ew *webView) {                                                      NSLog(@"Started loading %@", webView.request.URL.absoluteString);                                                  }                                                   shouldLoad:^BOOL(UIWebView *webView, UIWebViewNavigationType navigationType) {                                                       return YES;                                                   }];                  ```                  USAGE FOR HTML Strings                  -------                  ```objc                  UIWebView *webView = [UIWebView loadHTMLString:htmlString                                                          loaded:^(UIWebView *) {                                                              NSLog(@"Loaded successfully");                                                          }                                                          failed:^(UIWebView *, NSError *) {                                                               NSLog(@"Failed loading %@", error);                                                          }];                  ```                  **OR**                  ```objc                  UIWebView *webView = [UIWebView loadHTMLString:htmlString                                                          loaded:^(UIWebView *) {                                                              NSLog(@"Loaded successfully");                                                          }                                                          failed:^(UIWebView *, NSError *) {                                                               NSLog(@"Failed loading %@", error);                                                          }                                                     loadStarted:^(UIWebView *webView) {                                                         NSLog(@"Started loading %@", webView.request.URL.absoluteString);                                                     }        